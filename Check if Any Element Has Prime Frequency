#include <stdbool.h>

// Function to check if a number is prime
bool isPrime(int num) {
    if (num <= 1)
        return false;
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0)
            return false;
    }
    return true;
}

// LeetCode expects this function name
bool checkPrimeFrequency(int* nums, int numsSize) {
    int freq[101] = {0}; // Since nums[i] is in the range 0 to 100

    // Count frequency of each number
    for (int i = 0; i < numsSize; i++) {
        freq[nums[i]]++;
    }

    // Check if any frequency is a prime number
    for (int i = 0; i <= 100; i++) {
        if (isPrime(freq[i])) {
            return true;
        }
    }

    return false;
}
